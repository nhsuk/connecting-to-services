{
  "name": "gp-to-services",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "6.x.x"
  },
  "scripts": {
    "start": "node app.js",
    "lint": "eslint --ext .js,.json .",
    "lint-dev": "esw --watch .",
    "test": "mocha --recursive test",
    "test-dev": "npm run test -- --watch",
    "coverage-report": "istanbul cover _mocha -- --recursive test",
    "check-coverage": "istanbul check-coverage --config .istanbul.yml",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint && npm run test && npm run coverage-report && npm run check-coverage",
      "post-rewrite": "npm run lint && npm run test && npm run coverage-report && npm run check-coverage"
    }
  },
  "dependencies": {
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cookie-parser": "^1.3.3",
    "dotenv": "^2.0.0",
    "express": "^4.13.3",
    "json-query": "^2.1.0",
    "morgan": "^1.6.1",
    "nunjucks": "^2.4.2",
    "require-environment-variables": "^1.1.2",
    "valid-url": "^1.0.9",
    "xml2js": "^0.4.16"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "coveralls": "^2.11.9",
    "csv-parse": "^1.1.1",
    "eslint": "^3.0.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^2.0.0",
    "eslint-plugin-react": "^5.1.1",
    "eslint-watch": "^2.1.11",
    "ghooks": "^1.3.2",
    "grunt": "^1.0.1",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-develop": "^0.4.0",
    "istanbul": "^0.4.3",
    "load-grunt-tasks": "^3.2.0",
    "memory-cache": "^0.1.6",
    "mocha": "^2.5.3",
    "nock": "^8.0.0",
    "request": "^2.60.0",
    "time-grunt": "^1.2.1"
  }
}
