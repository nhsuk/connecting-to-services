{% extends 'layout.nunjucks' %}

{% block pageTitle %}{{getPlacesPageTitle()}}{% endblock %}

{% block meta %}
<meta name="DCSext.ServiceName" content="pharmacies" />
{% endblock %}

{% block content %}
